{
    "name": "mts88/laravel-zipkin",
    "version": "1.0.1",
    "type": "library",
    "description": "Zipkin Library for Laravel.",
    "keywords": [
        "zipkin",
        "laravel",
        "log",
        "openzipkin"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "openzipkin/zipkin": "^1.3.4"
    },
    "authors": [
        {
            "name": "Francesca Motisi",
            "email": "fsca.motisi@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mts88\\LaravelZipkin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mts88\\LaravelZipkin\\Providers\\LaravelZipkinServiceProvider"
            ],
            "aliases": {
                "Zipkin": "Mts88\\LaravelZipkin\\Facades\\Zipkin"
            }

        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
